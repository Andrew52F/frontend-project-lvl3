{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "import/extensions": "off",
      "no-console": "off",
      "no-underscore-dangle": [2, { "allow": ["__filename", "__dirname"] }],
      "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["watchedState"] }]
    }
}